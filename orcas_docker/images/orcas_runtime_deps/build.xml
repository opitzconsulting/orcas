<?xml version = '1.0' encoding = 'windows-1252'?>
<project name="load_mvn_deps" default="load_mvn_deps" >
  <property environment="env"/>
  <property name="orcas_dir" value="${env.ORCAS_CORE}"/>
  <import file="${orcas_dir}/orcas_default_tasks.xml"/>

  <target name="load_mvn_deps">
    <orcas_internal_gradle_build tmpfolder="/tmp/orcas_tmp" extensionfolder="" extensionparameter="" />
    <zip destfile="${env.ORCAS_PREBUILDFILE_NOEXTENSIONS}" >
      <fileset dir="/tmp/orcas_tmp/gradle_build" includes="*_distribution/**"/>
    </zip>
    <orcas_internal_gradle_build tmpfolder="/tmp/orcas_tmp" extensionfolder="${orcas_dir}/../orcas_domain_extension/extensions" extensionparameter="" />
    <zip destfile="${env.ORCAS_PREBUILDFILE_DOMAINEXTENSION}" >
      <fileset dir="/tmp/orcas_tmp/gradle_build" includes="*_distribution/**"/>
    </zip>
  </target>
</project>



